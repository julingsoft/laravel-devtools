{
    "name": "juling/laravel-devtools",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Juling\\DevTools\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\DevTools\\DevToolsServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
